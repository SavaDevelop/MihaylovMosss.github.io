                          li.nav-item
                              a.nav-link#credit-tab(data-toggle="tab" href="#credit" role="tab" aria-controls="credit" aria-selected="false") Кредит
                          li.nav-item
                              a.nav-link#postcredit-tab(data-toggle="tab" href="#postcredit" role="tab" aria-controls="postcredit" aria-selected="false") Рассрочка
